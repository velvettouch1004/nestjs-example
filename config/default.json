{
  "APP_SETTINGS": {
    "port": 8080,
    "bodyLimit": "50mb",
    "bodyParameterLimit": 50000
  },
  "CORS_SETTINGS": {
    "allowedOrigins": [],
    "allowedUrls": [],
    "allowedMethods": [
      "GET",
      "POST",
      "PUT",
      "PATCH",
      "DELETE",
      "OPTIONS"
    ],
    "allowedCredentials": false,
    "allowedHeaders": []
  },
  "LOGGER_SETTINGS": {
    "level": "info",
    "silence": [
      "healthz"
    ]
  },
  "DB_SETTINGS": {
    "host": "localhost",
    "port": 5432,
    "username": "postgres",
    "password": "postgres",
    "database": "nestjs_example",
    "logging": "all",
    "synchronize": true
  },
  "JWT_SETTINGS": {
    "secretKey": "secret_key",
    "algorithm": "HS512",
    "tokenExpiresIn": 20,
    "refreshTokenExpiresIn": 7
  },
  "REDIS_SETTINGS": {
    "host": "localhost",
    "port": 6379,
    "key": "nestjs_example"
  },
  "WSS_SETTINGS": {
    "port": 8081,
    "pingInterval": 3000,
    "pingTimeout": 10000,
    "path": "/socket.io"
  },
  "RABBITMQ_SETTINGS": {
    "exchange": "nestjs_example_service",
    "name": "rabbitmq",
    "host": "localhost",
    "vhost": "vhost",
    "port": 5672,
    "username": "admin",
    "password": "password"
  },
  "GRAPHQL_SETTINGS": {
    "playground": true,
    "debug": true,
    "introspection": true,
    "installSubscriptionHandlers": true
  },
  "GRAPHQL_APIS": {
    "api_1": "http://localhost:8090/graphql",
    "api_2": "http://localhost:8091/graphql"
  },
  "GRAPHQL_GUARD": {
    "username": "admin",
    "password": "password"
  }
}
