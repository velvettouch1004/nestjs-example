{
  "APP_SETTINGS": {
    "port": 8080,
    "body_limit": "50mb",
    "body_parameter_limit": 50000
  },
  "CORS_SETTINGS": {
    "allowed_origins": [],
    "allowed_paths": [],
    "allowed_methods": [
      "GET",
      "POST",
      "PUT",
      "PATCH",
      "DELETE",
      "OPTIONS"
    ],
    "allowed_credentials": false,
    "allowed_headers": []
  },
  "LOGGER_SETTINGS": {
    "level": "info",
    "silence": [
      "healthz"
    ]
  },
  "DB_SETTINGS": {
    "host": "localhost",
    "port": 5432,
    "username": "postgres",
    "password": "postgres",
    "database": "nestjs_example",
    "logging": "all",
    "synchronize": true
  },
  "JWT_SETTINGS": {
    "secret_key": "secret_key",
    "algorithm": "HS512",
    "access_token_expires_in": 20,
    "refresh_token_expires_in": 10080
  },
  "REDIS_SETTINGS": {
    "host": "localhost",
    "port": 6379,
    "key": "nestjs_example"
  },
  "WSS_SETTINGS": {
    "port": 8081,
    "ping_interval": 3000,
    "ping_timeout": 10000,
    "path": "/socket.io"
  },
  "RABBITMQ_SETTINGS": {
    "exchange": "nestjs_example_service",
    "name": "rabbitmq",
    "host": "localhost",
    "vhost": "vhost",
    "port": 5672,
    "username": "admin",
    "password": "password"
  },
  "GRAPHQL_SETTINGS": {
    "playground": true,
    "debug": true,
    "introspection": true,
    "installSubscriptionHandlers": true
  },
  "GRAPHQL_APIS": {
    "api_1": "http://localhost:8090/graphql",
    "api_2": "http://localhost:8091/graphql"
  },
  "GRAPHQL_GUARD": {
    "username": "admin",
    "password": "password"
  }
}
