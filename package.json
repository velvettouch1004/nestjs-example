{
  "name": "nestjs_example",
  "version": "0.3.5",
  "description": "nestjs_example",
  "author": "t.kosminov",
  "license": "MIT",
  "scripts": {
    "build": "rimraf dist && tsc -p tsconfig.build.json",
    "format": "prettier --write \"src/**/*.ts\"",
    "start": "ts-node --files -r tsconfig-paths/register src/main.ts",
    "start:debug": "nodemon --config nodemon-debug.json",
    "start:dev": "nodemon",
    "start:build": "NODE_ENV=development node dist/main.js",
    "start:beta": "NODE_ENV=beta node dist/main.js",
    "start:prod": "NODE_ENV=production node dist/main.js",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "lint-fix": "npm run lint -- --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js",
    "db:migration:create": "typeorm migration:create -d src/database/migrations -n"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint && git add -A"
    }
  },
  "dependencies": {
    "@nestjs-plus/common": "^1.1.0",
    "@nestjs-plus/rabbitmq": "^1.3.0",
    "@nestjs/common": "^6.5.2",
    "@nestjs/core": "^6.5.2",
    "@nestjs/graphql": "^6.3.0",
    "@nestjs/jwt": "^6.1.1",
    "@nestjs/passport": "^6.1.0",
    "@nestjs/platform-express": "^6.5.2",
    "@nestjs/platform-socket.io": "^6.5.2",
    "@nestjs/swagger": "^3.0.2",
    "@nestjs/typeorm": "^6.1.0",
    "@nestjs/websockets": "^6.5.2",
    "adm-zip": "^0.4.13",
    "amqplib": "^0.5.3",
    "apollo-link": "^1.2.12",
    "apollo-link-context": "^1.0.18",
    "apollo-link-http": "^1.5.15",
    "apollo-link-retry": "^2.2.14",
    "apollo-server-express": "^2.6.9",
    "bcryptjs": "^2.4.3",
    "class-transformer": "^0.2.3",
    "class-validator": "^0.9.1",
    "config": "^3.2.0",
    "cookie-parser": "^1.4.4",
    "dataloader": "^1.4.0",
    "graphql": "^14.4.2",
    "graphql-tools": "^4.0.5",
    "graphql-upload": "^8.0.7",
    "graphql-voyager": "^1.0.0-rc.27",
    "hasha": "^5.0.0",
    "helmet": "^3.18.0",
    "isomorphic-unfetch": "^3.0.0",
    "lodash": "^4.17.14",
    "passport": "^0.4.0",
    "passport-http-bearer": "^1.0.1",
    "passport-jwt": "^4.0.0",
    "pg": "^7.10.0",
    "reflect-metadata": "^0.1.12",
    "rimraf": "^2.6.2",
    "rxjs": "^6.5.1",
    "socket.io": "^2.2.0",
    "swagger-ui-express": "^4.0.7",
    "transliteration": "^2.1.4",
    "type-graphql": "^0.17.3",
    "typeorm": "^0.2.17"
  },
  "devDependencies": {
    "@babel/core": "^7.5.4",
    "@nestjs/testing": "^6.5.2",
    "@types/adm-zip": "^0.4.32",
    "@types/amqplib": "^0.5.13",
    "@types/bcryptjs": "^2.4.2",
    "@types/config": "0.0.34",
    "@types/cookie-parser": "^1.4.1",
    "@types/cors": "^2.8.5",
    "@types/express": "^4.17.0",
    "@types/helmet": "0.0.42",
    "@types/jest": "^23.3.1",
    "@types/node": "^11.13.17",
    "@types/passport-jwt": "^3.0.1",
    "@types/pg": "^7.4.14",
    "@types/socket.io": "^2.1.2",
    "@types/supertest": "^2.0.8",
    "husky": "^2.7.0",
    "jest": "^24.8.0",
    "nodemon": "^1.19.1",
    "prettier": "^1.18.2",
    "supertest": "^3.1.0",
    "ts-jest": "^23.1.3",
    "ts-loader": "^4.4.2",
    "ts-node": "^8.3.0",
    "tsconfig-paths": "^3.8.0",
    "tslint": "5.12.1",
    "tslint-clean-code": "^0.2.9",
    "tslint-config-prettier": "^1.18.0",
    "tslint-eslint-rules": "^5.4.0",
    "tslint-plugin-prettier": "^2.0.1",
    "typescript": "^3.5.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
